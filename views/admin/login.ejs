<!DOCTYPE html>
<html lang="en" dir="rtl">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="description" content="Seller Panel">
        <meta name="author" content="Coderthemes">

        <!-- App Favicon -->
        <link rel="shortcut icon" href="images/favicon.ico">

        <!-- App title -->
        <title>نمایشگاه صنایع دستی </title>

        <!-- App CSS -->
        <link href="/panel/css/bootstrap-rtl.min.css" rel="stylesheet" type="text/css" />
        <link href="/panel/css/core.css" rel="stylesheet" type="text/css" />
        <link href="/panel/css/components.css" rel="stylesheet" type="text/css" />
        <link href="/panel/css/icons.css" rel="stylesheet" type="text/css" />
        <link href="/panel/css/pages.css" rel="stylesheet" type="text/css" />
        <link href="/panel/css/menu.css" rel="stylesheet" type="text/css" />
        <link href="/panel/css/responsive.css" rel="stylesheet" type="text/css" />

        <!-- HTML5 Shiv and Respond.js IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
        <![endif]-->

        <script src="/panel/js/modernizr.min.js"></script>
        <script src="/admin/custom/js/apiURL.js"></script>
        
    </head>
    <body>

        <div class="account-pages"></div>
        <div class="clearfix"></div>
        <div class="wrapper-page">
            <div class="text-center">
                <a href="index.html" class="logo"><span>نمایشگاه <span>صنایع دستی</span></span></a>
                <h5 class="text-muted m-t-0 font-600">پنل سوپر ادمین</h5>
            </div>
        	<div class="m-t-40 card-box">
                <div class="text-center">
                    <h4 class="text-uppercase font-bold m-b-0">ورود</h4>
                </div>
                <div class="panel-body">
                    <form class="form-horizontal m-t-20" action="index.html">

                        <div class="form-group ">
                            <div class="col-xs-12">
                                <input name="email" id="email" class="form-control" type="text" required  placeholder="ایمیل" title="خواهشا فرم رو پر کنید" oninvalid="this.setCustomValidity('نام کاربری خود رو وارد کنید')"
                                oninput="this.setCustomValidity('')"  />
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-xs-12">
                                <input name="password" id="password" class="form-control" type="password" required="" placeholder="پسورد" oninvalid="this.setCustomValidity('لظفا رمز خود را وارد نمایید')"
                                oninput="this.setCustomValidity('')"  />
                            </div>
                        </div>

                        <div class="form-group text-center m-t-30">
                            <div class="col-xs-12">
                                <button id="loginButton" class="btn btn-custom btn-bordred btn-block waves-effect waves-light" type="submit">ورود</button>
                            </div>
                        </div>
                        </div>
                    </form>

                </div>
            </div>
            
        </div>
        <!-- end wrapper page -->
        

        
    	<script>
            var resizefunc = [];
        </script>

        <!-- jQuery  -->
        <script src="/panel/js/jquery.min.js"></script>
        <script src="/panel/js/bootstrap-rtl.min.js"></script>
        <script src="/panel/js/axios.js"></script>
        <script src="/admin/custom/js/login.js">
         </script>
        <script src="/admin/custom/js/ereaseSingleShop.js"></script>
        <script type="script">
            Document.querySelector('#loginButton').on
        </script>

        <!-- App js -->
        <script src="/panel/js/jquery.core.js"></script>
        <script src="/panel/js/jquery.app.js"></script>
	
	</body>
</html>