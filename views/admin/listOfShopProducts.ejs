        $("#datatable-responsive").DataTable();
        $("#datatable-scroller").DataTable({
          ajax: "assets/datatables/json/scroller-demo.json",
          deferRender: true,
          scrollY: 380,
          scrollCollapse: true,
          scroller: true,
        });
        var table = $("#datatable-fixed-header").DataTable({
