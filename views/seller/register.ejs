<!DOCTYPE html>
<html lang="en" dir="rtl">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="description" content="A fully featured admin theme which can be used to build CRM, CMS, etc.">
        <meta name="author" content="Coderthemes">

        <!-- App Favicon -->
        <link rel="shortcut icon" href="/panel/images/favicon.ico">

        <!-- App title -->
        <title>نمایشگاه صنایع دستی - پنل غرفه داران</title>

        <!-- App CSS -->
        <link href="/panel/css/bootstrap-rtl.min.css" rel="stylesheet" type="text/css" />
        <link href="/panel/css/core.css" rel="stylesheet" type="text/css" />
        <link href="/panel/css/components.css" rel="stylesheet" type="text/css" />
        <link href="/panel/css/icons.css" rel="stylesheet" type="text/css" />
        <link href="/panel/css/pages.css" rel="stylesheet" type="text/css" />
        <link href="/panel/css/menu.css" rel="stylesheet" type="text/css" />
        <link href="/panel/css/responsive.css" rel="stylesheet" type="text/css" />

        <!-- HTML5 Shiv and Respond.js IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
        <![endif]-->

        <script src="/panel/js/modernizr.min.js"></script>
        <script src="/seller/custom/js/apiURL.js"></script>

    </head>
    <body>

        <div class="account-pages"></div>
        <div class="clearfix"></div>
        <div class="wrapper-page">
            <div class="text-center">
                <a href="index.html" class="logo"><span>نمایشگاه <span>صنایع دستی</span></span></a>
                <h5 class="text-muted m-t-0 font-600">پنل غرفه داران</h5>
            </div>
        	<div class="m-t-40 card-box">
                <div class="text-center">
                    <h4 class="text-uppercase font-bold m-b-0">ثبت نام</h4>
                </div>
                <div class="panel-body">
                    <form class="form-horizontal m-t-20" action="index.html">

                        
                        <div class="form-group ">
                            <div class="col-xs-12">
                                <input id="registerFirstName" class="form-control" type="text" required="" placeholder="نام " oninvalid="this.setCustomValidity('لظفا  نام خود را وارد نمایید')" oninput="this.setCustomValidity('')">
							</div>
						</div>

                        <div class="form-group ">
                            <div class="col-xs-12">
                                <input id="registerLastName" class="form-control" type="text" required="" placeholder=" نام خانوادگی " oninvalid="this.setCustomValidity('لظفا  نام خانوادگی خود را وارد نمایید')" oninput="this.setCustomValidity('')">
							</div>
						</div>

                        <div class="form-group ">
                            <div class="col-xs-12">
                                <input id="registerEmail" class="form-control" type="email" required="" placeholder="ایمیل" oninvalid="this.setCustomValidity('لظفا  ایمیل خود را وارد نمایید')" oninput="this.setCustomValidity('')">
                            </div>
                        </div>
                        
						<div class="form-group">
							<div class="col-xs-12">
								<input id="registerPassword" class="form-control" type="password" required="" placeholder="رمز عبور" oninvalid="this.setCustomValidity('لظفا  رمز عبور خود را وارد نمایید')" oninput="this.setCustomValidity('')">
							</div>
						</div>
                        
						<div class="form-group">
							<div class="col-xs-12">
								<input id="registerReEnterPassword" class="form-control" type="password" required="" placeholder="تکرار رمز عبور" oninvalid="this.setCustomValidity('لظفا  تکرار رمز عبور خود را وارد نمایید')" oninput="this.setCustomValidity('')">
							</div>
						</div>

						<div class="form-group">
							<div class="col-xs-12">
								<div class="checkbox checkbox-custom">
									<input id="registerPolicy" type="checkbox" >
									<label for="checkbox-signup">من متن قوانین را خوانده <a href="#">و قبول میکنم</a></label>
								</div>
							</div>
						</div>

						<div class="form-group text-center m-t-40">
							<div class="col-xs-12">
								<button id="registerButton" class="btn btn-custom btn- vbordred btn-block waves-effect waves-light">
									ثبت نام
								</button>
							</div>
						</div>

					</form>

                </div>
            </div>
            <!-- end card-box -->

			<div class="row">
				<div class="col-sm-12 text-center">
					<p class="text-muted">اکانت کاربری داری?<a href="login" class="text-primary m-l-5"><b>وارد شو</b></a></p>
				</div>
			</div>

        </div>
        <!-- end wrapper page -->




    	<script>
            var resizefunc = [];
        </script>

        <!-- jQuery  -->
        <script src="/panel/js/jquery.min.js"></script>
        <script src="/panel/js/bootstrap-rtl.min.js"></script>
        <script src="/panel/js/detect.js"></script>
        <script src="/panel/js/fastclick.js"></script>
        <script src="/panel/js/jquery.slimscroll.js"></script>
        <script src="/panel/js/jquery.blockUI.js"></script>
        <script src="/panel/js/waves.js"></script>
        <script src="/panel/js/wow.min.js"></script>
        <script src="/panel/js/jquery.nicescroll.js"></script>
        <script src="/panel/js/jquery.scrollTo.min.js"></script>
        <script src="/panel/js/axios.js"></script>
        <script src="/seller/custom/js/register.js">
        </script>

        <!-- App js -->
        <script src="/panel/js/jquery.core.js"></script>
        <script src="/panel/js/jquery.app.js"></script>

	</body>
</html>